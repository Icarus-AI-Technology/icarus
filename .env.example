# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Optional: Claude API for IcarusBrain
VITE_CLAUDE_API_KEY=your_claude_api_key

# Optional: OpenAI API for additional AI features
VITE_OPENAI_API_KEY=your_openai_api_key

# Optional: Sentry Error Tracking
# Get your DSN from https://sentry.io
VITE_SENTRY_DSN=your_sentry_dsn
VITE_ENVIRONMENT=development
# Set to 'true' to force enable Sentry in development
# VITE_SENTRY_FORCE_ENABLE=false

# Figma Code Connect
# Required for publishing components to Figma
# Get your token from: Figma → Settings → Personal Access Tokens
# Scopes needed: File content (Read) + Code Connect (Write)
FIGMA_ACCESS_TOKEN=your_figma_personal_access_token
